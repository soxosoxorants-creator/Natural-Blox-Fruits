-- [[ FAST ATTACK ]]
task.spawn(function()
    while task.wait() do
        if _G.FastAttack and (_G.AutoFarm or _G.FarmNearest) then
            pcall(function()
                local tool = LP.Character:FindFirstChildOfClass("Tool")
                if tool then
                    RS.Remotes.Validator:FireServer(math.huge)
                    tool:Activate()
                end
            end)
        end
    end
end)

-- [[ AUTO STATS ]]
task.spawn(function()
    while task.wait(1.5) do
        if _G.AutoStats then
            local stat = _G.StatType == "Health" and "Defense" or _G.StatType
            RS.Remotes.CommF_:InvokeServer("AddPoint", stat, 3)
        end
    end
end)

-- [[ MAIN LOGIC ]]
task.spawn(function()
    while true do task.wait(0.1)
        pcall(function()
            if _G.AutoFarm or _G.FarmNearest or _G.AutoFactory or _G.AutoPirate then
                -- Global Noclip
                if LP.Character then
                    for _, v in pairs(LP.Character:GetChildren()) do
                        if v:IsA("BasePart") then v.CanCollide = false end
                    end
                end

                -- AUTO FARM LEVEL
                if _G.AutoFarm then
                    local myLvl = LP.Data.Level.Value
                    local info = nil
                    for i = #LevelData, 1, -1 do
                        if myLvl >= LevelData[i][1] then info = LevelData[i] break end
                    end

                    if info then
                        if not LP.PlayerGui.Main:FindFirstChild("QuestVisual") then
                            LP.Character.HumanoidRootPart.CFrame = info[4]
                            RS.Remotes.CommF_:InvokeServer("StartQuest", info[3], 1)
                        else
                            local mob = workspace.Enemies:FindFirstChild(info[2]) or workspace.NPCs:FindFirstChild(info[2])
                            if mob and mob:FindFirstChild("HumanoidRootPart") and mob.Humanoid.Health > 0 then
                                LP.Character.HumanoidRootPart.CFrame = mob.HumanoidRootPart.CFrame * CFrame.new(0, 11, 0)
                                local toolName = _G.WeaponType == "Blox Fruits" and "Fruit" or _G.WeaponType
                                local t = LP.Backpack:FindFirstChild(toolName) or LP.Character:FindFirstChild(toolName)
                                if t then LP.Character.Humanoid:EquipTool(t) end
                            else
                                -- HOVER SAFETY (45 Studs Above NPC)
                                LP.Character.HumanoidRootPart.CFrame = info[4] + Vector3.new(0, 45, 0)
                            end
                        end
                    end
                end

                -- FARM NEAREST
                if _G.FarmNearest then
                    local target, dist = nil, 2000
                    for _, v in pairs(workspace.Enemies:GetChildren()) do
                        if v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                            local d = (LP.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude
                            if d < dist then dist = d; target = v end
                        end
                    end
                    if target then LP.Character.HumanoidRootPart.CFrame = target.HumanoidRootPart.CFrame * CFrame.new(0, 10, 0) end
                end
            end
        end)
    end
end)

-- [[ UI ]]
local Tab1 = Window:CreateTab("Main Farm")
Tab1:CreateToggle({Name = "Auto Farm All Levels (0-2800)", CurrentValue = false, Callback = function(v) _G.AutoFarm = v end})
Tab1:CreateDropdown({Name = "Farm Focus", Options = {"Melee", "Sword", "Blox Fruits"}, CurrentOption = "Melee", Callback = function(v) _G.WeaponType = v end})
Tab1:CreateToggle({Name = "Fast Attack (Delta)", CurrentValue = false, Callback = function(v) _G.FastAttack = v end})
Tab1:CreateToggle({Name = "Farm Nearest NPC", CurrentValue = false, Callback = function(v) _G.FarmNearest = v end})

local Tab2 = Window:CreateTab("Auto Stats")
Tab2:CreateToggle({Name = "Enable Auto Stats", CurrentValue = false, Callback = function(v) _G.AutoStats = v end})
Tab2:CreateDropdown({Name = "Stat Category", Options = {"Melee", "Health", "Sword", "Blox Fruit"}, CurrentOption = "Melee", Callback = function(v) _G.StatType = v end})

local Tab3 = Window:CreateTab("World Events")
Tab3:CreateToggle({Name = "Auto Factory (Sea 2)", CurrentValue = false, Callback = function(v) _G.AutoFactory = v end})
Tab3:CreateToggle({Name = "Auto Pirate Raid (Sea 3)", CurrentValue = false, Callback = function(v) _G.AutoPirate = v end})

-- Anti-AFK
LP.Idled:Connect(function()
    game:GetService("VirtualUser"):Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    task.wait(1)
    game:GetService("VirtualUser"):Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)
