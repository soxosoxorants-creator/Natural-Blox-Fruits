local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

-- [[ THEME: LAVENDER 2026 ]]
local Window = Rayfield:CreateWindow({
    Name = "Natural Hub | BLOX FRUITS",
    LoadingTitle = "Natural Hub",
    LoadingSubtitle = "FEEDBACK ON CHANNEL",
    Theme = "Purple", 
    ConfigurationSaving = {Enabled = true, FileName = "NatHub_Lavender_Final"}
})

_G.AutoFarm = false
_G.WeaponType = "Melee" 
_G.FastAttack = false
_G.FarmNearest = false
_G.AutoStats = false
_G.StatType = "Melee"
_G.AutoFactory = false
_G.AutoPirate = false

local LP = game.Players.LocalPlayer
local RS = game:GetService("ReplicatedStorage")

-- [[ THE COMPLETE ROADMAP - 0 TO 2800 ]]
local LevelData = {
