local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

local Window = Rayfield:CreateWindow({
    Name = "Natural Hub | BLOX FRUITS",
    LoadingTitle = "Natural Hub v2800",
    LoadingSubtitle = "BLOX FRUITS NOOB",
    Theme = "Rose",
    ConfigurationSaving = {Enabled = true, FileName = "NatHub_Complete"}
})

_G.AutoFarm = false
_G.Weapon = "Melee"
_G.FastAttack = false
_G.FarmNearest = false
_G.AutoStats = false
_G.StatType = "Melee"
_G.StatAmount = 1
_G.AutoFactory = false
_G.AutoPirate = false

local LP = game.Players.LocalPlayer
local RS = game:GetService("ReplicatedStorage")

-- [[ THE COMPLETE LEVEL DATA - EVERY QUEST ]]
