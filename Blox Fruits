-- [[ FAST ATTACK ]]
task.spawn(function()
    while task.wait() do
        if _G.FastAttack and (_G.AutoFarm or _G.FarmNearest) then
            pcall(function()
                local tool = LP.Character:FindFirstChildOfClass("Tool")
                if tool then
                    RS.Remotes.Validator:FireServer(math.huge)
                    tool:Activate()
                end
            end)
        end
    end
end)

-- [[ MAIN AUTO FARM ]]
task.spawn(function()
    while true do task.wait(0.1)
        pcall(function()
            if _G.AutoFarm or _G.FarmNearest or _G.AutoFactory or _G.AutoPirate then
                -- Global Noclip
                for _, v in pairs(LP.Character:GetChildren()) do
                    if v:IsA("BasePart") then v.CanCollide = false end
                end

                if _G.AutoFarm then
                    local myLvl = LP.Data.Level.Value
                    local info = nil
                    for i = #LevelData, 1, -1 do
                        if myLvl >= LevelData[i][1] then info = LevelData[i] break end
                    end

                    if info then
                        if not LP.PlayerGui.Main:FindFirstChild("QuestVisual") then
                            LP.Character.HumanoidRootPart.CFrame = info[4]
                            RS.Remotes.CommF_:InvokeServer("StartQuest", info[3], 1)
                        else
                            local mob = workspace.Enemies:FindFirstChild(info[2]) or workspace.NPCs:FindFirstChild(info[2])
                            if mob and mob.Humanoid.Health > 0 then
                                LP.Character.HumanoidRootPart.CFrame = mob.HumanoidRootPart.CFrame * CFrame.new(0, 11, 0)
                                local t = LP.Backpack:FindFirstChild(_G.Weapon) or LP.Character:FindFirstChild(_G.Weapon)
                                if t then LP.Character.Humanoid:EquipTool(t) end
                            else
                                -- HOVER SAFETY (Prevents Ocean TP)
                                LP.Character.HumanoidRootPart.CFrame = info[4] + Vector3.new(0, 45, 0)
                            end
                        end
                    end
                end
            end
        end)
    end
end)

-- [[ UI ]]
local T1 = Window:CreateTab("Auto Grind")
T1:CreateToggle({Name = "Auto Farm (ALL LEVELS)", CurrentValue = false, Callback = function(v) _G.AutoFarm = v end})
T1:CreateDropdown({Name = "Weapon Type", Options = {"Melee", "Sword"}, CurrentOption = "Melee", Callback = function(v) _G.Weapon = v end})
T1:CreateToggle({Name = "Fast Attack (Delta Fix)", CurrentValue = false, Callback = function(v) _G.FastAttack = v end})

T1:CreateSection("Character Stats")
T1:CreateToggle({Name = "Auto Stats", CurrentValue = false, Callback = function(v) _G.AutoStats = v end})
T1:CreateDropdown({Name = "Stat Type", Options = {"Melee", "Defense", "Sword", "Blox Fruit"}, CurrentOption = "Melee", Callback = function(v) _G.StatType = v end})

local T2 = Window:CreateTab("World Events")
T2:CreateToggle({Name = "Factory (Sea 2)", CurrentValue = false, Callback = function(v) _G.AutoFactory = v end})
T2:CreateToggle({Name = "Pirate Raid (Sea 3)", CurrentValue = false, Callback = function(v) _G.AutoPirate = v end})

-- Stats Loop
task.spawn(function()
    while task.wait(2) do if _G.AutoStats then RS.Remotes.CommF_:InvokeServer("AddPoint", _G.StatType, 1) end end
end)

-- Anti-AFK
LP.Idled:Connect(function()
    game:GetService("VirtualUser"):Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    task.wait(1)
    game:GetService("VirtualUser"):Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)
